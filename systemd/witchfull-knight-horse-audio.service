[Unit]
Description=Witchfull Knight Horse Audio
After=network-online.target

[Service]
Type=simple
User=tempus
WorkingDirectory=/home/tempus/witchfull-knight-horse-audio
ExecStart=/usr/bin/node /home/tempus/witchfull-knight-horse-audio/index.mjs
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
